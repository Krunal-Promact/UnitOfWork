@using UnitOfWorkDemo.Models;
@model List<Student>
@{
    ViewBag.Title = "Index";
}
<h2>Student Lists</h2>

<table class="table table-hover">
    <tr>
        <th>
            Name
        </th>
        <th>
            Email
        </th>
        <th>
            Number
        </th>
    </tr>
    @foreach (var student in Model)
    {
        <tr>
            <td>
                @student.Name
            </td>
            <td>
                @student.Email
            </td>
            <td>
                @student.Number
            </td>
        </tr>
    }
</table>